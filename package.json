{
  "name": "ember-project-deprecation-dashboard",
  "version": "0.1.2",
  "private": false,
  "description": "A dashboard tool to keep track of Ember Project Deprecations",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "bin": {
    "ember-project-deprecation-dashboard": "./cli.js"
  },
  "scripts": {
    "build:tailwind": "tailwindcss -i ./tailwind-input.css -o ./public/assets/tailwind.css",
    "build:ember": "ember build -e production",
    "build": "npm run build:tailwind && npm run build:ember",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "ember:serve": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "start": "concurrently \"npm run tailwind:watch\" \"npm run ember:serve\"",
    "tailwind:watch": "tailwindcss -i ./tailwind-input.css -o ./public/assets/tailwind.css --watch",
    "prepublishOnly": "rm -rf dist && npm run build"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.8.1",
    "@fortawesome/ember-fontawesome": "^0.4.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^7.6.0",
    "ember-auto-import": "^2.5.0",
    "ember-cli": "~4.9.2",
    "ember-cli-app-version": "^5.0.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-htmlbars": "^6.1.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~4.9.1",
    "ember-fetch": "^8.1.2",
    "ember-get-config": "^2.1.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.0.0",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.9.1",
    "ember-template-lint": "^5.2.0",
    "ember-truth-helpers": "^3.1.1",
    "ember-welcome-page": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "loader.js": "^4.7.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.1",
    "qunit": "^2.19.3",
    "qunit-dom": "^2.0.0",
    "tailwindcss": "^3.2.4",
    "webpack": "^5.75.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "dependencies": {
    "commander": "^9.5.0",
    "ember-project-deprecation-analysis": "^0.1.1",
    "fs-extra": "^11.1.0"
  }
}
